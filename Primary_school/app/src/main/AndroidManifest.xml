            android:screenOrientation="portrait" ></activity>
        <activity android:name=".Framgment.shequchildactivity.ShezhiActivity" />
        <activity android:name=".basequickadapter.Main2Activity" />
        <activity android:name=".Coor4Activity" />
        <activity android:name=".lunbotu.LunbotuActivity" />
        <activity android:name=".AutoCompleteTextView.Main3Activity">
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:persistableMode="persistAcrossReboots"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>